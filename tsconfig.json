{
   "compilerOptions": {
      "strict": true,
      "lib": [
         "ES5",
         "ES2015",
         "ES2016",
         "ES2018",
         "ES2019",
         "ES2020",
      ],
      "target": "ES2020",
      "module": "CommonJS",
      "moduleResolution": "Node",
      "newLine": "lf",
      "allowJs": true,
      "checkJs": true,
      "noEmit": true,
   },

   "exclude": [
      "dist",
      "node_modules",
   ]
}
